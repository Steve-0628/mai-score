{
  "name": "mai-score",
  "version": "1.0.0",
  "description": "maimaiでらっくすのスコア管理ツール",
  "author": "mnao305",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "dev": "cross-env NUXT_HOST=0.0.0.0 nuxt-ts",
    "build": "cross-env NODE_ENV=production nuxt-ts build",
    "build-dev": "cross-env NODE_ENV=development nuxt-ts build --devtools",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "build:firebase": "npm run clean && npm run build && npm run copy",
    "build-dev:firebase": "npm run clean && npm run build-dev && npm run copy",
    "clean": "rm -rf public && rm -rf .nuxt",
    "copy": "mkdir public && cp -R functions public/server && cp -R client/static public/client && cp -R .nuxt public/server && cp -R public/server/.nuxt/dist/client public/client/assets"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "0.3.7",
    "@nuxtjs/axios": "5.9.3",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/google-analytics": "2.2.3",
    "@nuxtjs/pwa": "2.6.0",
    "cookie-universal-nuxt": "2.1.0",
    "firebase": "7.6.2",
    "js-cookie": "2.2.1",
    "nuxt": "2.11.0",
    "nuxt-logrocket": "1.2.5",
    "ts-node": "8.6.1",
    "vue-property-decorator": "8.3.0",
    "vuex-persistedstate": "2.7.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "0.5.6",
    "@nuxtjs/eslint-config": "0.0.1",
    "@nuxtjs/eslint-config-typescript": "1.0.0",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/vuetify": "1.9.1",
    "@types/js-cookie": "2.2.4",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "@vue/test-utils": "1.0.0-beta.30",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "cross-env": "6.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-nuxt": "0.5.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.1.2",
    "jest": "24.9.0",
    "node-sass": "4.13.0",
    "nodemon": "1.19.4",
    "prettier": "1.19.1",
    "sass": "1.24.4",
    "sass-loader": "7.3.1",
    "vue-jest": "3.0.5"
  }
}
