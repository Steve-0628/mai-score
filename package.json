{
  "name": "mai-score",
  "version": "1.0.0",
  "description": "maimaiでらっくすのスコア管理ツール",
  "author": "mnao305",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "dev": "cross-env NUXT_HOST=0.0.0.0 nuxt-ts",
    "build": "cross-env NODE_ENV=production nuxt-ts build",
    "build-dev": "cross-env NODE_ENV=development nuxt-ts build --devtools",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "build:firebase": "npm run clean && npm run build && npm run copy",
    "build-dev:firebase": "npm run clean && npm run build-dev && npm run copy",
    "clean": "rm -rf public && rm -rf .nuxt",
    "copy": "mkdir public && cp -R functions public/server && cp -R client/static public/client && cp -R .nuxt public/server && cp -R public/server/.nuxt/dist/client public/client/assets"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.1.3",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "cookie-universal-nuxt": "^2.0.16",
    "firebase": "^6.3.0",
    "js-cookie": "^2.2.0",
    "nuxt": "^2.9.2",
    "ts-node": "^8.3.0",
    "vue-property-decorator": "^8.2.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.1.11",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-config-typescript": "^0.1.2",
    "@nuxtjs/eslint-module": "^0.0.1",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/js-cookie": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "sass": "^1.22.10",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.3"
  }
}
